<html>
    <head>
        <title>Iowa Western Community College</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    </head>
    <body>
        {{> partials/header }}
        <main class="p-home">
            
            <section class="s-hero">
                <div class="hero-caption">
                    <p>alt description</p>
                    <h2>The world is waiting</h2> 
                    <p>
                        <span class="play-button">Play Video <span class="icon-play"></span></span>   
                    </p>
                </div>
            </section>
            
            <section class="s-finder">
                <h2 class="medallion">Find your Way</h2>
                <div class="left-nav">
                    <a href="#">Academic Programs</a>
                    <a href="#" class="active">Apply Now</a>
                    <a href="#">Cost &amp; Tuition</a>
                </div>
                <div class="right-nav">
                    <a href="#">Financial Aid</a>
                    <a href="#" class="active">Schedule a visit</a>
                    <a href="#">What Will You Become?</a>
                </div>
            </section>

        <section class="content">
            <section class="s-newsevents">
                <div class="news">
                    <div class="intro">
                        <h3>IWTV Weekly news</h3>
                        <a href="/more/" class="btn-next">More News</a>
                    </div>
                    <div class="newsfeed">
                        <div class="feed">

                        </div>
                    </div>
                </div>
                <div class="events">
                    <div class="intro">
                        <h3>Upcoming Events</h3>
                        <a href="/more/" class="btn-next">Full Calendar</a>
                    </div>
                    <div class="event-day-list">
                        <div class="event-day">
                            <div class="date">
                                <span class="date-month">MAR</span>
                                <span class="date-day">11</span>
                            </div>
                            <ul class="event-list">
                                <li>
                                    <h4><a href="#">Board of Trustees Meeting</a></h4>
                                    <p>2PM - 4PM</p>
                                </li>
                                <li>
                                    <h4><a href="#">Spring accelerated Session 2 Begins</a></h4>
                                </li>
                            </ul>
                        </div>
                        <div class="event-day">
                            <div class="date">
                                <span class="date-month">MAR</span>
                                <span class="date-day">12</span>
                            </div>
                            <ul class="event-list">
                                <li>
                                    <h4><a href="#">Board of Trustees Meeting</a></h4>
                                    <p>2PM - 4PM</p>
                                </li>
                                <li>
                                    <h4><a href="#">Spring accelerated Session 2 Begins</a></h4>
                                </li>
                            </ul>
                        </div>
                        <div class="event-day">
                            <div class="date">
                                <span class="date-month">MAR</span>
                                <span class="date-day">13</span>
                            </div>
                            <ul class="event-list">
                                <li>
                                    <h4><a href="#">Board of Trustees Meeting</a></h4>
                                    <p>2PM - 4PM</p>
                                </li>
                                <li>
                                    <h4><a href="#">Spring accelerated Session 2 Begins</a></h4>
                                </li>
                            </ul>
                        </div>
                        <div class="event-day">
                            <div class="date">
                                <span class="date-month">MAR</span>
                                <span class="date-day">14</span>
                            </div>
                            <ul class="event-list">
                                <li>
                                    <h4><a href="#">Board of Trustees Meeting</a></h4>
                                    <p>2PM - 4PM</p>
                                </li>
                                <li>
                                    <h4><a href="#">Spring accelerated Session 2 Begins</a></h4>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>

            <section class="s-admissions">
                <div class="admissions-nav-container">
                    <h2>Admissions</h2>
                    <ul class="admissions-nav">
                        <li>
                            <a href="/apply/" class="btn btn-action">
                                Apply
                                <div class="ribbon"><span class="subhead">It's Free</span></div>
                            </a>
                        </li>
                        <li>
                            <a href="/apply/" class="btn btn-action">
                                Visit
                                <div class="ribbon"><span class="subhead">Join Us</span></div>
                            </a>
                        </li>
                        <li>
                            <a href="/apply/" class="btn btn-action">
                                Register
                                <div class="ribbon"><span class="subhead">Sign Up</span></div>
                            </a>
                        </li>
                        <li>
                            <a href="/apply/" class="btn btn-action">
                                Graduate
                                 <div class="ribbon"><span class="subhead">The World is Waiting</span></div>
                           </a>
                        </li>
                    </ul>
                </div>

                <div class="admissions-panels">
                    <div class="admissions-card">
                        <div class="become-a-reiver ">
                            <button class="play">Become a Reiver</button>
                        </div>
                    </div>
                    <div class="admissions-card">
                        <div class="campus-map">
                            <button>Campus Map and Tour</button>
                        </div>
                    </div>
                </div>
            </section>

            <section class="s-academics">
                <div class="academics-container">
                    <h2>Academics</h2>
                    <div class="btn-list">
                        <button type="button" class="btn btn-list-item business">Business</button>
                        <button type="button" class="btn btn-list-item industrial">Industrial Technology<br>and Transportation</button>
                        <button type="button" class="btn btn-list-item communication">Communication<br>and Fine Arts</button>
                        <button type="button" class="btn btn-list-item health">Health</button>
                        <button type="button" class="btn btn-list-item science">Science, Technology,<br>Engineering and Math</button>
                        <button type="button" class="btn btn-list-item social">Social and<br>Behavioral Sciences</button>
                    </div>
                </div> 
            </section>

            <section class="s-featured">
                <div class="featured-container">
                    <div class="featured-athletics featured-panel col-md-4">
                        <img src="/images/home/reivers-logo.png" width="100%" class="img-fluid">
                        <a href="https://GoReivers.com" target="_blank" class="btn">GoReivers.com</a>
                    </div>
                    <div class="featured-artscenter featured-panel col-md-4">
                            <img src="/images/home/artscenter-logo.png" width="100%" class="img-fluid">
                        <a href="https://artscenter.iwcc.edu" target="_blank" class="btn">artscenter.iwcc.edu</a>
                    </div>
                    <div class="featured-river featured-panel col-md-4">
                            <img src="/images/home/river-logo.png" width="100%" class="img-fluid">
                        <a href="897theriver.com" target="_blank" class="btn">897theriver.com</a>
                    </div>
            </div>
            </section>

            <section class="s-centers">
                    <h2>Centers</h2>
                    <div class="center-panels">
                        <div class="center-panel">
                            <a href="" class="btn clarinda">Clarinda Center</a>
                            <div class="center-image">
                                <img src="/images/home/center-image.png" class="img-fluid">
                            </div>
                        </div>
                            <div class="center-panel">
                            <a href="" class="btn cass">Cass County Center</a>
                            <div class="center-image">
                                <img src="/images/home/center-image.png" class="img-fluid">
                            </div>
                        </div>
                            <div class="center-panel">
                            <a href="" class="btn shelby">Shelby County Center</a>
                            <div class="center-image">
                                <img src="/images/home/center-image.png" class="img-fluid">
                            </div>
                        </div>
                            <div class="center-panel">
                            <a href="" class="btn fremont">Page/Fremont County Center</a>
                            <div class="center-image">
                                <img src="/images/home/center-image.png" class="img-fluid">
                            </div>
                        </div>
                    </div>
            </section>
        </section>
        </main>
        {{> partials/footer }}
    </body>
</html>